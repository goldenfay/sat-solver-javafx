<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="700.0" prefWidth="450.0" styleClass="mainFxmlClass" stylesheets="@../Styles/ga_settings_gui.css" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Controllers.Graph_GUIController">
   <children>
        <VBox alignment="center" fillWidth="true" prefHeight="700.0" prefWidth="450.0" spacing="0" stylesheets="@../Styles/graph_gui.css">
         <children>
            <BarChart fx:id="barChart" stylesheets="@../Styles/ga_gui.css" title="statistics">
              <xAxis>
                <CategoryAxis side="BOTTOM" />
              </xAxis>
              <yAxis>
                <NumberAxis side="LEFT" />
              </yAxis>
            </BarChart>
         </children>
      </VBox>
   </children>
</AnchorPane>
